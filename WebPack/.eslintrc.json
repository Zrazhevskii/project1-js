{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "no-console": "off", // ошибка - Unexpected console statement - Неожиданный оператор консоли
        "linebreak-style": 0,
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
        ],
        "import/extensions": [ // ошибка Unexpected use of file extension "js" for "./js/app.js" - неожиданное использование расширение файла js
            "error",
            {
                "js": "ignorePackages" 
            }
        ]
    }
}
